extends layout

block content
  div(class="container" ng-controller="AppCtrl")
    h1 AngulAir
    div(class="pull-left span6")
      ul
        li(ng-repeat="airport in airports")
          a(href="" ng-click="setAirport(airport.code)") {{airport.code}} - {{airport.city | uppercase}}
    ng-include(class="span5", src="sidebarPartial")
